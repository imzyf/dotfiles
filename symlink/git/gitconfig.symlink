# This is Git's per-user configuration file.
[user]
    name = yifan
    email = zhaoyifans@gmail.com
[core]
    excludesfile = ~/.gitignore_global
[push]
    default = current
[commit]
	template = ~/.stCommitMsg
[includeIf "gitdir:~/Documents/code/"]
    path = ~/Documents/code/.gitconfig
[alias]
    co = checkout
    br = branch
    st = status
    me = merge
    ci = commit